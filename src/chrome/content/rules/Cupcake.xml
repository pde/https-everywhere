<ruleset name="Cupcake">
    <target host="cupcake.io" />
    <target host="*.cupcake.io" />
    <target host="cupcake.is" />
    <target host="*.cupcake.is" />

    <rule from="^http://([\w-]+\.)?cupcake\.io/" to="https://$1cupcake.io/" />
    <rule from="^http://([\w-]+\.)?cupcake\.is/" to="https://$1cupcake.is/" />
</ruleset>
